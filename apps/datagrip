#!/bin/sh
#Generated by JetBrains Toolbox 1.19.7784 at 2021-01-05T08:20:14.682459

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/jarod/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/201.8538.4/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"